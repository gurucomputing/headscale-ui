		"autoprefixer": "^10.4.4",
		"daisyui": "^2.19.0",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.0.23",
		"typescript": "^4.7.2",
